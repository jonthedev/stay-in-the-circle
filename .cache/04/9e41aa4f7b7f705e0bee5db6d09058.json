:"Component","generated":{"line":103,"column":17},"source":"src/components/Circle.js","original":{"line":3,"column":27}},{"name":"Circle","generated":{"line":105,"column":15},"source":"src/components/Circle.js","original":{"line":51,"column":15}}],"sources":{"src/components/Circle.js":"import React from \"react\";\n\nclass Circle extends React.Component {\n  constructor() {\n    super();\n    this.state = {\n      circle: {\n        inside: false,\n      },\n    };\n  }\n\n  circleStatus = () => {\n    //set status if user is inside the circle\n    this.setState(prevState => ({\n      circle: {\n        inside: !prevState.circle.inside,\n      },\n    }));\n\n    const { inside } = this.state.circle;\n\n    //check if game has started & user is within the circle\n    if (!inside && this.props.gameStatus) {\n      this.props.addCount();\n    }\n  };\n\n  render() {\n    const gameStatus = this.props.gameStatus;\n    return (\n      <svg viewBox=\"0,0 10,10\" width=\"250px\" height=\"250px\">\n        <path\n          className=\"track\"\n          fill=\"none\"\n          strokeWidth=\"0.25\"\n          d=\"M 5 5 m -4, 0 a 4,4 0 1,0 8,0 a 4,4 0 1,0 -8,0\"\n        />\n        <circle\n          className={gameStatus ? \"marker marker-rotate\" : \"marker\"}\n          onMouseOver={() => this.circleStatus()}\n          onMouseLeave={() => this.circleStatus()}\n          r=\"0.6\"\n          fill=\"orange\"\n        ></circle>\n      </svg>\n    );\n  }\n}\n\nexport default Circle;\n"},"lineCount":null}},"error":null,"hash":"48e1df33b05ec000e5a532f8465caebf","cacheData":{"env":{}}}